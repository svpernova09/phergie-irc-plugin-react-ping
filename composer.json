{
    "name": "renegade334/phergie-irc-plugin-react-ping",
    "description": "Phergie plugin for verifying that the client connection has not been dropped by sending self CTCP PINGs after periods of inactivity.",
    "keywords": [ "phergie", "irc", "bot", "plugin" ],
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "Renegade334",
            "homepage": "http://www.renegade334.me.uk/",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "team@phergie.org",
        "irc": "irc://irc.freenode.net/phergie",
        "issues": "https://github.com/Renegade334/phergie-irc-plugin-react-ping/issues",
        "source": "https://github.com/Renegade334/phergie-irc-plugin-react-ping"
    },
    "require": {
        "phergie/phergie-irc-bot-react": "~2"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.6",
        "phake/phake": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "Renegade334\\Phergie\\Plugin\\React\\Ping\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Renegade334\\Phergie\\Tests\\Plugin\\React\\Ping\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
